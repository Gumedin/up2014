
Процедура ПередЗаписью(Отказ, Замещение)
	Для Н = 1 ПО Количество() Цикл
		ТекЗапись = Получить( Н - 1 );
		Если ТекЗапись.Работа.Суммарная ИЛИ ТекЗапись.ВедущаяРабота.Суммарная Тогда
			Сообщение = Новый СообщениеПользователю;
			Сообщение.Текст = "Работа " + ТекЗапись.Работа + Символы.ПС + 
							  "или " + ТекЗапись.ВедущаяРабота + Символы.ПС + 
							  "являются суммарными";
			Сообщение.Сообщить();
							
			Отказ = Истина;
			Возврат;
		КонецЕсли;
	КонецЦикла;
		
КонецПроцедуры
