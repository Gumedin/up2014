		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	ТРВ.Ссылка,
	ТРВ.ДатаТабеля,
	ТРВ.Начало КАК НачалоРаботы,
	ТРВ.ОтметкаТабеля,
	ТРВ.КоличествоЧасов,
	ТРВ.СодержаниеРабот,
	ВЫБОР
		КОГДА ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.ПроектыТиражные.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.Проекты.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Документ.АктТиражногоПодразделения.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Документ.ПланРабот.ПустаяСсылка)
				ИЛИ ТРВ.Основание = НЕОПРЕДЕЛЕНО
			ТОГДА ТРВ.ОтметкаТабеля.Наименование
		ИНАЧЕ ТРВ.Основание.Ссылка
	КОНЕЦ КАК Проект,
	ТРВ.Ссылка.ФизическоеЛицо,
	ТРВ.Ссылка.Подразделение,
	ТРВ.Ссылка.Организация,
	ВЫБОР
		КОГДА ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.ПроектыТиражные.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.Проекты.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Документ.ПланРабот.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Документ.АктТиражногоПодразделения.ПустаяСсылка)
				ИЛИ ТРВ.Основание = НЕОПРЕДЕЛЕНО
			ТОГДА "Офис"
		КОГДА ТИПЗНАЧЕНИЯ(ТРВ.Основание) = ТИП(Справочник.ПроектыТиражные)
			ТОГДА "Тиражный проект"
		КОГДА ТИПЗНАЧЕНИЯ(ТРВ.Основание) = ТИП(Справочник.Проекты)
			ТОГДА "Проект"
		КОГДА ТИПЗНАЧЕНИЯ(ТРВ.Основание) = ТИП(Справочник.Контрагенты)
			ТОГДА "Контрагент"
		КОГДА ТИПЗНАЧЕНИЯ(ТРВ.Основание) = ТИП(Документ.АктТиражногоПодразделения)
			ТОГДА "Акт"
		КОГДА ТИПЗНАЧЕНИЯ(ТРВ.Основание) = ТИП(Документ.ПланРабот)
			ТОГДА "План работ"
		ИНАЧЕ "не указано"
	КОНЕЦ КАК ТипПроекта,
	ВЫБОР
		КОГДА ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.Контрагенты.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.ПроектыТиражные.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Справочник.Проекты.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Документ.АктТиражногоПодразделения.ПустаяСсылка)
				ИЛИ ТРВ.Основание = ЗНАЧЕНИЕ(Документ.ПланРабот.ПустаяСсылка)
				ИЛИ ТРВ.Основание = НЕОПРЕДЕЛЕНО
			ТОГДА ""
		КОГДА ТИПЗНАЧЕНИЯ(ТРВ.Основание) = ТИП(Документ.ПланРабот)
			ТОГДА ТРВ.Основание.ЗадачаПроекта.Владелец
		ИНАЧЕ ""
	КОНЕЦ КАК Примечание
ИЗ
	Документ.ТабельУчетаРабочегоВремени.РабочееВремя КАК ТРВ
ГДЕ
	ТРВ.Ссылка.Проведен
	И ТРВ.ДатаТабеля МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ТРВ.Ссылка.Подразделение = &amp;Подразделение</query>
