		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ПТР.ПланРаботТиражный.ЗадачаПроекта.Владелец.МенеджерПроекта КАК КлиентМенеджер,
	ПТР.ПланРаботТиражный.Подразделение КАК Подразделение,
	ПТР.ПланРаботТиражный.ЗадачаПроекта.Владелец КАК Проект,
	ПТР.Номенклатура,
	ПТР.ПланРаботТиражный КАК ПланРабот,
	СУММА(ВЫБОР
			КОГДА ПТР.ВидДвижения = &amp;Приход
				ТОГДА ПТР.Количество
			ИНАЧЕ 0
		КОНЕЦ) КАК План,
	СУММА(ВЫБОР
			КОГДА ПТР.ВидДвижения = &amp;Приход
				ТОГДА ПТР.Количество * ПТР.Цена
			ИНАЧЕ 0
		КОНЕЦ) КАК ПланСумма,
	СУММА(ВЫБОР
			КОГДА ПТР.ВидДвижения = &amp;Расход
				ТОГДА ПТР.Количество * ПТР.Цена
			ИНАЧЕ 0
		КОНЕЦ) КАК ФактСумма,
	СУММА(ВЫБОР
			КОГДА ПТР.ВидДвижения = &amp;Расход
				ТОГДА ПТР.Количество
			ИНАЧЕ 0
		КОНЕЦ) КАК Факт,
	НАЧАЛОПЕРИОДА(ПТР.Месяц, МЕСЯЦ) КАК Месяц,
	СУММА(ВЫБОР
			КОГДА ПТР.ВидДвижения = &amp;Расход
				ТОГДА ПТР.Скидка
			ИНАЧЕ 0
		КОНЕЦ) КАК Скидка
ИЗ
	РегистрНакопления.ПланТиражныхРабот КАК ПТР
ГДЕ
	(&amp;ВсеМенеджеры
			ИЛИ ПТР.ПланРаботТиражный.ЗадачаПроекта.Владелец.МенеджерПроекта В (&amp;Менеджеры))
	И (&amp;ВсеПодразделения
			ИЛИ ПТР.ПланРаботТиражный.Подразделение В (&amp;Подразделения))
	И ПТР.Месяц МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И ПТР.ПланРаботТиражный.ЗадачаПроекта.Владелец.Статус В(&amp;Статус)

СГРУППИРОВАТЬ ПО
	ПТР.ПланРаботТиражный,
	ПТР.Номенклатура,
	ПТР.ПланРаботТиражный.ЗадачаПроекта.Владелец.МенеджерПроекта,
	ПТР.ПланРаботТиражный.Подразделение,
	ПТР.ПланРаботТиражный.ЗадачаПроекта.Владелец,
	НАЧАЛОПЕРИОДА(ПТР.Месяц, МЕСЯЦ)</query>
