		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	СУММА(ВЫБОР
			КОГДА ДанныеПроизводственногоКалендаря.ВидДня = ЗНАЧЕНИЕ(Перечисление.ВидыДнейПроизводственногоКалендаря.Рабочий)
				ТОГДА 8
			КОГДА ДанныеПроизводственногоКалендаря.ВидДня = ЗНАЧЕНИЕ(Перечисление.ВидыДнейПроизводственногоКалендаря.Предпраздничный)
				ТОГДА 7
			ИНАЧЕ 0
		КОНЕЦ) КАК РабочихЧасов,
	НАЧАЛОПЕРИОДА(ДанныеПроизводственногоКалендаря.Дата, МЕСЯЦ) КАК Месяц
ПОМЕСТИТЬ ВТ_РабочихЧасов
ИЗ
	РегистрСведений.ДанныеПроизводственногоКалендаря КАК ДанныеПроизводственногоКалендаря
ГДЕ
	ДанныеПроизводственногоКалендаря.ПроизводственныйКалендарь = &amp;ПроизводственныйКалендарь

СГРУППИРОВАТЬ ПО
	НАЧАЛОПЕРИОДА(ДанныеПроизводственногоКалендаря.Дата, МЕСЯЦ)
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	СотрудникиСрезПоследних.Подразделение,
	СотрудникиСрезПоследних.ТарифнаяСтавка,
	СУММА(СотрудникиСрезПоследних.Количество) КАК Ставок
ПОМЕСТИТЬ ВТ_СтавкиПодразделений
ИЗ
	РегистрСведений.Сотрудники.СрезПоследних(&amp;НачалоГода, ) КАК СотрудникиСрезПоследних
ГДЕ
	НЕ СотрудникиСрезПоследних.Подразделение = ЗНАЧЕНИЕ(Справочник.Подразделения.ПустаяСсылка)
	И СотрудникиСрезПоследних.СтатусСотрудника.Активный

СГРУППИРОВАТЬ ПО
	СотрудникиСрезПоследних.ТарифнаяСтавка,
	СотрудникиСрезПоследних.Подразделение
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ФР.Месяц,
	ФР.ТарифнаяСтавка,
	СУММА(ФР.Количество) КАК Количество,
	СУММА(ФР.Сумма) КАК Сумма,
	ФР.Ссылка.Подразделение,
	ФР.Ссылка.ЗадачаПроекта,
	ФР.Ссылка.ЗадачаПроекта.Владелец КАК Проект,
	ВЫБОР
		КОГДА ВТ_СтавкиПодразделений.Ставок ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ ВТ_СтавкиПодразделений.Ставок
	КОНЕЦ КАК Ставок,
	ВТ_РабочихЧасов.РабочихЧасов КАК РЧ_в_Месяц,
	ФР.Ссылка КАК ПланРабот
ИЗ
	Документ.ПланРабот.ФронтРабот КАК ФР
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_РабочихЧасов КАК ВТ_РабочихЧасов
		ПО ФР.Месяц = ВТ_РабочихЧасов.Месяц
		ЛЕВОЕ СОЕДИНЕНИЕ ВТ_СтавкиПодразделений КАК ВТ_СтавкиПодразделений
		ПО ФР.ТарифнаяСтавка = ВТ_СтавкиПодразделений.ТарифнаяСтавка
			И ФР.Ссылка.Подразделение = ВТ_СтавкиПодразделений.Подразделение
ГДЕ
	ФР.Ссылка.Проведен
	И ФР.Ссылка.ЗадачаПроекта.Владелец.ГодПроекта = &amp;ГодПроекта
	И (&amp;ВсеПодразделения
			ИЛИ ФР.Ссылка.Подразделение В (&amp;Подразделение))
	И (&amp;ВсеТарифныеСтавки
			ИЛИ ФР.ТарифнаяСтавка В (&amp;ТарифныеСтавки))
	И (&amp;ВсеПроекты
			ИЛИ ФР.Ссылка.ЗадачаПроекта.Владелец В (&amp;Проекты))
	И ФР.Ссылка.ЗадачаПроекта.Владелец.Статус В(&amp;Статусы)

СГРУППИРОВАТЬ ПО
	ФР.Ссылка,
	ФР.ТарифнаяСтавка,
	ФР.Месяц,
	ФР.Ссылка.Подразделение,
	ФР.Ссылка.ЗадачаПроекта,
	ФР.Ссылка.ЗадачаПроекта.Владелец,
	ВТ_РабочихЧасов.РабочихЧасов,
	ВЫБОР
		КОГДА ВТ_СтавкиПодразделений.Ставок ЕСТЬ NULL 
			ТОГДА 0
		ИНАЧЕ ВТ_СтавкиПодразделений.Ставок
	КОНЕЦ</query>
