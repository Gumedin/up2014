		<dataSource>ИсточникДанных1</dataSource>
		<query>ВЫБРАТЬ
	ТП.Ссылка.ПериодРегистрации КАК Месяц,
	ТП.Ссылка.Подразделение,
	ТП.ФизическоеЛицо,
	ТП.Должность,
	ТП.Основание КАК ПланРабот,
	ТП.ТарифнаяСтавка,
	СУММА(ТП.КоличествоТРВ) КАК КоличествоТРВ,
	СУММА(ТП.КоличествоТЗП) КАК КоличествоТЗП,
	СУММА(ТП.Количество) КАК Разница
ИЗ
	Документ.ТабельПодразделения.РабочееВремя КАК ТП
ГДЕ
	ТП.Ссылка.Проведен
	И ТП.Ссылка.ПериодРегистрации МЕЖДУ &amp;НачалоПериода И &amp;КонецПериода
	И (&amp;ВсеПодразделения
			ИЛИ ТП.Ссылка.Подразделение В ИЕРАРХИИ (&amp;Подразделение))
	И (&amp;ВсеПланыРабот
			ИЛИ ТП.Основание = &amp;ПланРабот)

СГРУППИРОВАТЬ ПО
	ТП.Основание,
	ТП.ФизическоеЛицо,
	ТП.Должность,
	ТП.ТарифнаяСтавка,
	ТП.Ссылка.ПериодРегистрации,
	ТП.Ссылка.Подразделение</query>
