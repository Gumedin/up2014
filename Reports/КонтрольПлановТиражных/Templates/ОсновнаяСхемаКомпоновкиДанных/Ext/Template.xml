	СУММА(ЕСТЬNULL(ОстаткиТирАиПОстаткиИОбороты.СуммаНачальныйОстаток, 0)) КАК НачОстаток,
	СУММА(ЕСТЬNULL(ОстаткиТирАиПОстаткиИОбороты.СуммаКонечныйОстаток, 0)) КАК КонОстаток,
	СУММА(ЕСТЬNULL(ОстаткиТирАиПОстаткиИОбороты.СуммаПриход, 0)) КАК Лимит,
	СУММА(ЕСТЬNULL(ОстаткиТирАиПОстаткиИОбороты.СуммаРасход, 0)) КАК Закрыто,
	ВТ_ПроектыАкты.План КАК План,
	ВТ_ПроектыАкты.ПланРаботТиражный.Подразделение КАК Подразделение,
	ВТ_ПроектыАкты.ПланРаботТиражный.ЗадачаПроекта.Владелец КАК Проект,
	ВТ_ПроектыАкты.ПланРаботТиражный.ЗадачаПроекта.Владелец.МенеджерПроекта КАК МенеджерПроекта,
	ВТ_ПроектыАкты.ОстатокПлана КАК ОстатокПлана
{ВЫБРАТЬ
	ПланРаботТиражный.*,
	АктПроект.*,
	НачОстаток,
	КонОстаток,
	Лимит,
	Закрыто,
	План,
	Подразделение.*,
	Проект.*,
	МенеджерПроекта.*,
	ОстатокПлана}
ИЗ
	ВТ_ПроектыАкты КАК ВТ_ПроектыАкты
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОстаткиТирАиП.ОстаткиИОбороты(, {(&amp;ДатаОтчета)}, , , ) КАК ОстаткиТирАиПОстаткиИОбороты
		ПО ВТ_ПроектыАкты.ПланРаботТиражный = ОстаткиТирАиПОстаткиИОбороты.ПланРабот
			И ВТ_ПроектыАкты.АктПроект = ОстаткиТирАиПОстаткиИОбороты.Источник
{ГДЕ
	ВТ_ПроектыАкты.ПланРаботТиражный.*,
	ВТ_ПроектыАкты.АктПроект.*,
	ВТ_ПроектыАкты.ПланРаботТиражный.Подразделение.* КАК Подразделение,
	ВТ_ПроектыАкты.ПланРаботТиражный.ЗадачаПроекта.Владелец.* КАК Проект,
	ВТ_ПроектыАкты.ПланРаботТиражный.ЗадачаПроекта.Владелец.МенеджерПроекта.* КАК МенеджерПроекта}

