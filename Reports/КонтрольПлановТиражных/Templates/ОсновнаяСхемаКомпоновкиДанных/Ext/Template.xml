		<query>ВЫБРАТЬ
	ПланРаботТиражный.Ссылка КАК ПланРаботТиражный,
	СУММА(ЕСТЬNULL(ФронтРаботОбороты.СуммаОборот, 0)) КАК План,
	СУММА(ЕСТЬNULL(ФронтРаботОбороты.СуммаОборот, 0) - ЕСТЬNULL(ОстаткиТирАиПОбороты.СуммаПриход, 0)) КАК ОстатокПлана
ПОМЕСТИТЬ ВТ_ПланыРабот
ИЗ
	Документ.ПланРаботТиражный КАК ПланРаботТиражный
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ФронтРабот.Обороты(, , , ТипСуммы = 0) КАК ФронтРаботОбороты
		ПО (ФронтРаботОбороты.ПланРабот = ПланРаботТиражный.Ссылка)
		ЛЕВОЕ СОЕДИНЕНИЕ РегистрНакопления.ОстаткиТирАиП.Обороты(, {(&amp;ДатаОтчета)}, , ) КАК ОстаткиТирАиПОбороты
		ПО (ОстаткиТирАиПОбороты.ПланРабот = ПланРаботТиражный.Ссылка)
ГДЕ
	ПланРаботТиражный.ЗадачаПроекта.Владелец.ГодПроекта = &amp;ГодПроекта
	И ПланРаботТиражный.Проведен

СГРУППИРОВАТЬ ПО
	ПланРаботТиражный.Ссылка
;

////////////////////////////////////////////////////////////////////////////////
ВЫБРАТЬ
	ВТ_ПланыРабот.ПланРаботТиражный,
	ПроектыТиражные.Ссылка КАК АктПроект,
	ВТ_ПланыРабот.План,
	ВТ_ПланыРабот.ОстатокПлана
ПОМЕСТИТЬ ВТ_ПроектыАкты
ИЗ
	ВТ_ПланыРабот КАК ВТ_ПланыРабот
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Справочник.ПроектыТиражные КАК ПроектыТиражные
		ПО ВТ_ПланыРабот.ПланРаботТиражный = ПроектыТиражные.ПланРабот

СГРУППИРОВАТЬ ПО
	ВТ_ПланыРабот.ПланРаботТиражный,
	ПроектыТиражные.Ссылка,
	ВТ_ПланыРабот.План,
	ВТ_ПланыРабот.ОстатокПлана

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТ_ПланыРабот.ПланРаботТиражный,
	АктТиражногоПодразделения.Ссылка,
	ВТ_ПланыРабот.План,
	ВТ_ПланыРабот.ОстатокПлана
ИЗ
	ВТ_ПланыРабот КАК ВТ_ПланыРабот
		ВНУТРЕННЕЕ СОЕДИНЕНИЕ Документ.АктТиражногоПодразделения КАК АктТиражногоПодразделения
		ПО ВТ_ПланыРабот.ПланРаботТиражный = АктТиражногоПодразделения.ПланРабот
ГДЕ
	АктТиражногоПодразделения.Дата &lt;= &amp;ДатаОтчета

СГРУППИРОВАТЬ ПО
	ВТ_ПланыРабот.ПланРаботТиражный,
	АктТиражногоПодразделения.Ссылка,
	ВТ_ПланыРабот.План,
	ВТ_ПланыРабот.ОстатокПлана
